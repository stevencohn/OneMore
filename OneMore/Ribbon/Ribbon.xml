<?xml version="1.0" encoding="utf-8" ?>
<customUI
  xmlns="http://schemas.microsoft.com/office/2009/07/customui"
	onLoad="RibbonLoaded"
	loadImage="GetRibbonImage">
  <ribbon>
    <tabs/>
  </ribbon>
  <contextMenus>
    <contextMenu idMso="ContextMenuFileAttachment">
      <button
        id="ctxCaptionAttachmentsButton"
        imageMso="CaptionInsert"
        getLabel="GetRibbonLabel"
        getScreentip="GetRibbonScreentip"
        onAction="CaptionAttachmentsCmd"
        insertBeforeMso="FilePrintoutInsert"/>
    </contextMenu>
    <contextMenu idMso="ContextMenuNotebook">
      <button
        id="ctxArchiveNotebookButton"
        imageMso="SaveAttachments"
        getLabel="GetRibbonLabel"
        getScreentip="GetRibbonScreentip"
        onAction="ArchiveCmd"
        tag="notebook"
        insertBeforeMso="ShareThisNotebook"/>
      <button
        id="ctxNumberSectionsButton"
        imageMso="LineNumbersMenu"
        getLabel="GetRibbonLabel"
        getScreentip="GetRibbonScreentip"
        onAction="NumberSectionsCmd"
        insertBeforeMso="ShareThisNotebook"/>
      <button
        id="ctxRemoveSectionNumbersButton"
        imageMso="HeaderFooterRemoveHeaderWord"
        getLabel="GetRibbonLabel"
        getScreentip="GetRibbonScreentip"
        onAction="RemoveSectionNumbersCmd"
        insertBeforeMso="ShareThisNotebook"/>
      <button
        id="ctxSortNotebookSectionsButton"
        imageMso="SortUp"
        getLabel="GetRibbonLabel"
        getScreentip="GetRibbonScreentip"
        onAction="SortSectionsCmd"
        insertBeforeMso="ShareThisNotebook"/>
      <button
        id="ctxNotebookWordCountButton"
        imageMso="WordCount"
        getLabel="GetRibbonLabel"
        getScreentip="GetRibbonScreentip"
        onAction="NotebookWordCountCmd"
        insertBeforeMso="ShareThisNotebook"/>
      <button
        id="ctxViewNotebookInBrowserButton"
        imageMso="OpenInBrowserOutlook"
        getLabel="GetRibbonLabel"
        getScreentip="GetRibbonScreentip"
        onAction="ViewNotebookInBrowserCmd"
        insertBeforeMso="ShareThisNotebook"/>
      <menuSeparator
        id="omNotebookContextMenuSeparator"
        insertBeforeMso="ShareThisNotebook"/>
    </contextMenu>
    <contextMenu idMso="ContextMenuNotebookArea">
      <button
        id="ctxSortNotebooksButton"
        imageMso="SortUp"
        getLabel="GetRibbonLabel"
        getScreentip="GetRibbonScreentip"
        onAction="SortNotebooksCmd"
        insertBeforeMso="SectionInsert"/>
      <menuSeparator
        id="omNotebooksContextMenuSeparator"
        insertBeforeMso="SectionInsert"/>
    </contextMenu>
    <contextMenu idMso="ContextMenuPage">
      <button
        id="ctxDuplicatePageButton"
        imageMso="PasteDuplicate"
        getLabel="GetRibbonLabel"
        getScreentip="GetRibbonScreentip"
        onAction="DuplicatePageCmd"
        insertBeforeMso="RenamePageOneNote"/>
      <button
        id="ctxExportButton"
        imageMso="FileSave"
        getLabel="GetRibbonLabel"
        getScreentip="GetRibbonScreentip"
        onAction="ExportCmd"
        insertBeforeMso="RenamePageOneNote"/>
      <button
        id="ctxMergeButton"
        imageMso="CompareAndCombine"
        getLabel="GetRibbonLabel"
        getScreentip="GetRibbonScreentip"
        onAction="MergeCmd"
        insertBeforeMso="RenamePageOneNote"/>
      <button
        id="ctxMovePageTopButton"
        imageMso="UpArrow2"
        getLabel="GetRibbonLabel"
        getScreentip="GetRibbonScreentip"
        onAction="MovePageTopCmd"
        insertBeforeMso="RenamePageOneNote"/>
      <button
        id="ctxMovePageBottomButton"
        imageMso="DownArrow2"
        getLabel="GetRibbonLabel"
        getScreentip="GetRibbonScreentip"
        onAction="MovePageBottomCmd"
        insertBeforeMso="RenamePageOneNote"/>
      <button
        id="ctxPageColorButton"
        imageMso="ShapeFillColorPicker"
        getLabel="GetRibbonLabel"
        getScreentip="GetRibbonScreentip"
        onAction="PageColorCmd"
        insertBeforeMso="RenamePageOneNote"/>
