        id="ctxStylizeImagesButton"
        imageMso="GroupPictureStyles"
        getLabel="GetRibbonLabel"
        getScreentip="GetRibbonScreentip"
        onAction="StylizeImagesCmd"
        insertBeforeMso="Cut"/>
      <menuSeparator
        id="omPictureContextMenuSeparator"
        insertBeforeMso="Cut"/>
