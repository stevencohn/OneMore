        id="ctxSplitButton"
        imageMso="MasterDocumentSplitSubdocuments"
        getLabel="GetRibbonLabel"
        getScreentip="GetRibbonScreentip"
        onAction="SplitCmd"
        insertBeforeMso="RenamePageOneNote"/>
      <button
        id="ctxSortPageButton"
        imageMso="SortUp"
        getLabel="GetRibbonLabel"
        getScreentip="GetRibbonScreentip"
        onAction="SortPageCmd"
        insertBeforeMso="RenamePageOneNote"/>
      <button
        id="ctxViewPageInBrowserButton"
        imageMso="OpenInBrowserOutlook"
        getLabel="GetRibbonLabel"
        getScreentip="GetRibbonScreentip"
        onAction="ViewPageInBrowserCmd"
        insertBeforeMso="RenamePageOneNote"/>
      <menuSeparator
        id="omPageContextMenuSeparator"
        insertBeforeMso="RenamePageOneNote"/>
    </contextMenu>
    <contextMenu idMso="ContextMenuPageArea">
      <button
        id="ctxNumberPagesButton"
        imageMso="CustomPageNumberGallery"
        getLabel="GetRibbonLabel"
        getScreentip="GetRibbonScreentip"
        onAction="NumberPagesCmd"
        insertBeforeMso="RenamePageOneNote"/>
      <button
        id="ctxRemovePageNumbersButton"
        imageMso="PageNumbersRemove"
        getLabel="GetRibbonLabel"
        getScreentip="GetRibbonScreentip"
        onAction="RemovePageNumbersCmd"
        insertBeforeMso="RenamePageOneNote"/>
      <button
        id="ctxSortPagesButton"
        imageMso="SortUp"
        getLabel="GetRibbonLabel"
        getScreentip="GetRibbonScreentip"
        onAction="SortPagesCmd"
        insertBeforeMso="RenamePageOneNote"/>
    </contextMenu>
    <contextMenu idMso="ContextMenuOutlineHandle">
      <button
        id="ctxArrangeContainersButton"
        imageMso="WindowsArrangeAll"
        getLabel="GetRibbonLabel"
        getScreentip="GetRibbonScreentip"
        onAction="ArrangeContainersCmd"
        insertBeforeMso="Cut"/>
      <menuSeparator
        id="omOutlineContextMenuSeparator"
        insertBeforeMso="Cut"/>
    </contextMenu>
    <contextMenu idMso="ContextMenuPicture">
      <button
        id="ctxAddCaptionButton"
        imageMso="CaptionInsert"
        getLabel="GetRibbonLabel"
        getScreentip="GetRibbonScreentip"
        onAction="AddCaptionCmd"
        insertBeforeMso="Cut"/>
      <button
        id="ctxCropImageButton"
        imageMso="PictureCrop"
        getLabel="GetRibbonLabel"
        getScreentip="GetRibbonScreentip"
        onAction="CropImageCmd"
        insertBeforeMso="Cut"/>
      <button
        id="ctxOpenImageWithButton"
        imageMso="PictureInsertFromFile"
        getLabel="GetRibbonLabel"
        getScreentip="GetRibbonScreentip"
        onAction="OpenImageWithCmd"
        insertBeforeMso="Cut"/>
      <button
        id="ctxAdjustImagesButton"
        imageMso="GroupPictureCompress"
        getLabel="GetRibbonLabel"
        getScreentip="GetRibbonScreentip"
        onAction="AdjustImagesCmd"
        insertBeforeMso="Cut"/>
