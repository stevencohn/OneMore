        id="ctxArchiveSectionButton"
        imageMso="SaveAttachments"
        getLabel="GetRibbonLabel"
        getScreentip="GetRibbonScreentip"
        onAction="ArchiveCmd"
        tag="section"
        insertBeforeMso="SectionRenameOneNote"/>
      <button
        id="ctxDateStampButton"
        imageMso="DateAndTimeInsert"
        getLabel="GetRibbonLabel"
        getScreentip="GetRibbonScreentip"
        onAction="DateStampCmd"
        tag="section"
        insertBeforeMso="SectionRenameOneNote"/>
      <button
        id="ctxSectionColorButton"
        imageMso="ShapeFillColorPicker"
        getLabel="GetRibbonLabel"
        getScreentip="GetRibbonScreentip"
        onAction="SectionColorCmd"
        insertBeforeMso="SectionRenameOneNote"/>
      <button
        id="ctxRemoveDateStampButton"
        imageMso="RemoveCitation"
        getLabel="GetRibbonLabel"
        getScreentip="GetRibbonScreentip"
        onAction="RemoveDateStampCmd"
        tag="section"
        insertBeforeMso="SectionRenameOneNote"/>
      <button
        id="ctxSortSectionPagesButton"
        imageMso="SortUp"
        getLabel="GetRibbonLabel"
        getScreentip="GetRibbonScreentip"
        onAction="SortPagesCmd"
        insertBeforeMso="SectionRenameOneNote"/>
      <button
        id="ctxViewSectionInBrowserButton"
        imageMso="OpenInBrowserOutlook"
        getLabel="GetRibbonLabel"
        getScreentip="GetRibbonScreentip"
        onAction="ViewSectionInBrowserCmd"
        insertBeforeMso="SectionRenameOneNote"/>
      <button
        id="ctxSectionWordCountButton"
        imageMso="WordCount"
        getLabel="GetRibbonLabel"
        getScreentip="GetRibbonScreentip"
        onAction="SectionWordCountCmd"
        insertBeforeMso="SectionRenameOneNote"/>
      <menuSeparator
        id="omSectionContextMenuSeparator"
        insertBeforeMso="SectionRenameOneNote"/>
    </contextMenu>
    <contextMenu idMso="ContextMenuSectionArea">
      <button
        id="ctsArchiveNotebookButton"
        imageMso="SaveAttachments"
        getLabel="GetRibbonLabel"
        getScreentip="GetRibbonScreentip"
        onAction="ArchiveCmd"
        tag="notebook"
        insertBeforeMso="SectionInsert"/>
      <button
        id="ctsNumberSectionsButton"
        imageMso="LineNumbersMenu"
        getLabel="GetRibbonLabel"
        getScreentip="GetRibbonScreentip"
        onAction="NumberSectionsCmd"
        insertBeforeMso="SectionInsert"/>
      <button
        id="ctsRemoveSectionNumbersButton"
        imageMso="HeaderFooterRemoveHeaderWord"
        getLabel="GetRibbonLabel"
        getScreentip="GetRibbonScreentip"
        onAction="RemoveSectionNumbersCmd"
        insertBeforeMso="SectionInsert"/>
      <button
        id="ctsSortNotebookSectionsButton"
        imageMso="SortUp"
        getLabel="GetRibbonLabel"
        getScreentip="GetRibbonScreentip"
        onAction="SortSectionsCmd"
        insertBeforeMso="SectionInsert"/>
      <menuSeparator
        id="omSectionAreaContextMenuSeparator"
        insertBeforeMso="SectionInsert"/>
    </contextMenu>
    <contextMenu idMso="ContextMenuSectionGroup">
      <button
        id="ctxCopyFolderButton"
        imageMso="CopyFolder"
        getLabel="GetRibbonLabel"
        getScreentip="GetRibbonScreentip"
        onAction="CopyFolderCmd"
        insertBeforeMso="FolderRename"/>
      <button
        id="ctxSortSectionsInGroupButton"
        imageMso="SortUp"
        getLabel="GetRibbonLabel"
        getScreentip="GetRibbonScreentip"
        onAction="SortSectionsInGroupCmd"
        insertBeforeMso="FolderRename"/>
      <menuSeparator
        id="omSectionGroupContextMenuSeparator"
        insertBeforeMso="FolderRename"/>
    </contextMenu>
  </contextMenus>
</customUI>
